{
  "name": "apicloud-vue-ide",
  "version": "0.0.9",
  "description": "apicloud vue 自定义框架",
  "scripts": {
    "web": "yarn createEntryConfig && cross-env BUILD_ENV=web webpack-dev-server --hot --config build/webpack.web.js",
    "dev": "yarn createEntryConfig && cross-env BUILD_ENV=development webpack --watch --config build/webpack.dev.js",
    "build": "yarn createEntryConfig && cross-env BUILD_ENV=production webpack --config build/webpack.prod.js",
    "lint": "eslint --fix --ext .js,.vue ./src/",
    "createEntryConfig": "node build/entryConfigCreate.js"
  },
  "author": "yaniky",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.5.0",
